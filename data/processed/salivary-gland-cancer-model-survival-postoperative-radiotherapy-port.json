efault": 0, "conditionality": "", "show_points": false, "tips_en": "", "optional": false, "options": [{"label": "Negative", "value": 0}, {"label": "Positive", "value": 1}], "name": "eight"}, {"type": "radio", "label_en": "Primary site", "default": 0, "conditionality": "", "show_points": false, "tips_en": "", "optional": false, "options": [{"label": "Sublingual", "value": 1}, {"label": "Submandibular", "value": 2}, {"label": "Parotid", "value": 0}], "name": "nine"}, {"type": "radio", "label_en": "Postoperative radiotherapy (PORT)", "default": 0, "conditionality": "", "show_points": false, "tips_en": "", "optional": false, "options": [{"label": "No", "value": 0}, {"label": "Yes", "value": 1}], "name": "two"}], "md5": "", "cmeVersion": null, "cmeReleaseDate": null, "cmeEndDate": null, "cmeLastReviewed": null, "related_calcs": [{"calcId": 3925, "short_title_en": "Fong Score", "slug": "fong-clinical-risk-score-colorectal-cancer-recurrence"}, {"calcId": 3974, "short_title_en": "TNM Melanoma Staging", "slug": "tnm-staging-melanoma"}, {"calcId": 10210, "short_title_en": "PREVAIL Model for Prostate Cancer", "slug": "prevail-model-prostate-cancer-survival"}]}